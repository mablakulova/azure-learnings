{
    "AppInsightsSettings": {
        "Enabled": true,
        "ConnectionString": "INSIGHTS_CONNECTION_STRING"
    },
    "AuthenticationSettings": {
        "JwtIssuer": "AzureBlobManager",
        "JwtAudience": "AzureBlobManager",
        "TokenExpirationSeconds": 86400,
        "JwtSigningKeyBase64": "JWT_SIGNING_KEY_BASE64"
    },
    "AzureBlobStorageSettings": {
        "ConnectionString": "AZURE_BLOB_CONNECTION_STRING",
        "ContainerName": "AZURE_BLOB_CONTAINER_NAME"
    },
    "AzureKeyVaultSettings": {
        "AddToConfiguration": true,
        "ServiceUrl": "KEY_VAULT_SERVICE_URL"
    },
    "AzureServiceBusSettings": {
        "ConnectionString": "AZURE_BUS_CONNECTION_STRING",
        "QueueName": "AZURE_BUS_QUEUE_NAME"
    },
    "CorsSettings": {
        "AllowedOrigins": [
            "https://localhost:7190"
        ]
    },
    "DbConnectionStrings": {
        "DefaultConnection": "DB_DEFAULT_CONNECTION"
    },
    "SwaggerSettings": {
        "ApiName": "AzureBlobManager",
        "UseSwagger": true,
        "LoginPath": "/account/oauth2/access_token"
    }
}